<Window x:Class="NanoTwitchLeafs.Windows.TriggerHelpWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NanoTwitchLeafs.Windows"
        xmlns:p="clr-namespace:NanoTwitchLeafs.Properties"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.Window_TriggerHelp_Title}" Height="691" Width="880.7" ResizeMode="NoResize">
    <Grid>
        <TextBlock Margin="10,10,10,0" TextWrapping="Wrap"><Label Content="{x:Static p:Resources.Window_TriggerHelp_Text}" /></TextBlock>
    </Grid>
</Window>